{
  "name": "<%= pluginName %>",
  "version": "0.1.0",
  "main": "src/index.js",
  "license": "<%= license %>",
  "keywords": [
    "invision",
    "studio",
    "plugin",
    "<%= appname %>"
  ],
  "description": "<%= description %>",
  "author": {
    "name": "<%= name %>",
    "email": "<%= email %>"
  },
  "scripts": {
    "build:development": "webpack --config ./webpack.config.js --mode development",
    "build:production": "webpack --config ./webpack.config.js --mode production",
    "build": "npm run build:production",
    "format": "prettier \"**/*.@(js?(x)|json|md)\" --write",
    "lint": "eslint . --ext .js,.jsx",
    "start": "npm run build:development -- --watch",
    "test": "npm run lint && jest"
  },
  "engines": {
    "node": "8.x || 10.x",
    "npm": "5.x || 6.x"
  },
  "config": {
    "ghooks": {
      "pre-push": "npm run githook"
    }
  },
  "devDependencies": {
    "@invisionapp/studio-api": "^3.0.0",
    "babel-cli": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^5.0.2",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "2.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.5.1",
    "eslint": "^4.12.1",
    "file-loader": "^1.1.11",
    "ghooks": "2.0.4",
    "jest": "23.4.1",
    "mkdirp": "^0.5.1",
    "node-fetch": "^2.2.0",
    "prettier": "1.13.7",
    "react-dom": "^15.6.2",
    "react": "15.6.2",
    "rimraf": "^2.6.3",
    "uglifyjs-webpack-plugin": "1.2.7",
    "webpack-cli": "3.1.0",
    "webpack": "4.16.1"
  }
}
