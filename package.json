{
  "name": "generator-studio-plugin",
  "version": "0.4.1",
  "description": "Yeoman generator for InVision Studio Plugins",
  "homepage": "https://www.invisionapp.com/studio",
  "author": {
    "name": "InVision, Inc."
  },
  "files": [
    "generators"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org/",
    "access": "public"
  },
  "main": "generators/index.js",
  "keywords": [
    "yeoman-generator",
    "invision",
    "studio",
    "invision studio"
  ],
  "devDependencies": {
    "eslint": "^5.10.0",
    "eslint-config-prettier": "^3.3.0",
    "eslint-config-xo": "^0.25.1",
    "eslint-plugin-prettier": "^3.0.0",
    "husky": "^1.2.0",
    "jest": "^23.6.0",
    "lint-staged": "^8.1.0",
    "nsp": "^3.2.1",
    "prettier": "^1.15.3",
    "yeoman-assert": "^3.1.1",
    "yeoman-test": "^1.9.1"
  },
  "engines": {
    "npm": ">= 5.0.0"
  },
  "dependencies": {
    "chalk": "^2.4.1",
    "yeoman-generator": "^3.1.1",
    "yosay": "^2.0.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "./coverage/",
    "testEnvironment": "node"
  },
  "scripts": {
    "prepublishOnly": "npm audit --audit-level high",
    "pretest": "eslint .",
    "precommit": "lint-staged",
    "test": "jest"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "xo",
      "prettier"
    ],
    "env": {
      "jest": true,
      "node": true
    },
    "rules": {
      "prettier/prettier": [
        "error",
        {
          "singleQuote": true,
          "printWidth": 90
        }
      ]
    },
    "plugins": [
      "prettier"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/InVisionApp/generator-studio-plugin"
  },
  "license": "MIT"
}
